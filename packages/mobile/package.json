{
  "name": "mobile",
  "version": "0.12.12",
  "private": true,
  "workspaces": {
    "nohoist": [
      "mobx-react-lite"
    ]
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "clean": "sh ./scripts/clean.sh",
    "android": "yarn build:background && yarn build:provider && echo 'Building Android' && expo run:android",
    "ios": "yarn build:background && yarn build:provider && echo 'Building iOS' && expo run:ios",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "typecheck": "tsc",
    "build:background": "echo 'Building build:background' && yarn fs:script && sh ./scripts/fix-background-module.sh && yarn workspace @keplr-wallet/background build",
    "build:provider": "echo 'Building build:provider' && tsc --project tsconfig.provider.json && browserify -p tinyify build/injected/index.js -o build/injected/injected-provider.bundle.js && cp -rf build/injected/injected-provider.bundle.js android/app/src/main/assets/.",
    "fs:script": "sh ./scripts/fix-fetchai-blst-js.sh && sh ./scripts/fix-fetchai-path-node-js.sh",
    "start": "expo start --dev-client",
    "start:reset": "expo start --dev-client --reset-cache",
    "generate:splash-image": "expo-splash-screen --background-color \"#FBF8FF\" --image-path ./src/assets/logo/splash-screen-only-k.png --image-resize-mode contain --status-bar-translucent --status-bar-background-color \"#FBF8FF\" --status-bar-style dark-content",
    "pod-install": "pod-install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "sh ./scripts/fix-android-splash-screen-flash.sh && sh ./scripts/fix-ios-blur-view.sh && sh ./scripts/force-export-ble-manager-from-ledger.sh && sh ./scripts/fix-android-legacy-dependency.sh"
  },
